<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TiTsEd"
                    xmlns:common="clr-namespace:TiTsEd.Common">

    <BitmapImage x:Key="CrossImage" UriSource="../Assets/cross.png" />

    <BitmapImage x:Key="SearchIcon" UriSource="../Assets/search.png" />

    <BitmapImage x:Key="PlusIcon" UriSource="../Assets/plus.png" />

    <Style x:Key="smallImageStyleKey" TargetType="Image">
        <Setter Property="Width" Value="16" />
        <Setter Property="Height" Value="16" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
    </Style>

    <common:UriConverter x:Key="UriConverter" />

</ResourceDictionary>
